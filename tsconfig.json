{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ES2020",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "declaration": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "module": "Node18",
    "moduleResolution": "Node16",
  },
  "include": ["src"],
}
